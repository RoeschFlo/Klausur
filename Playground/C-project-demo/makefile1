# PROJECT:     Shell Script Course
# DESCRITPION: Build the C-demo-project
# LANGUAGE:    make

.PHONY: all clean

CC=gcc
CFLAGS=-Wall -O2 -I .
LD=gcc

all: main.exe

main.o : main.c main.h module1.h general.h
	$(CC) $(CFLAGS) -c main.c
	
module1.o : module1.c module1.h general.h
	$(CC) $(CFLAGS) -c module1.c
	
main.exe: main.o module1.o
	$(LD) -o main.exe main.o module1.o
	ls -l main.exe

clean: 
	rm main.exe main.o module1.o
